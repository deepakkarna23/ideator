<!DOCTYPE html>
<html>
<head>
  <title>Workspace</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  
  <link href="https://fonts.googleapis.com/css?family=Dosis|Open+Sans:300,400,600,700" rel="stylesheet">
  <link rel="stylesheet" type="text/css" href="/app/assets/stylesheet/master.css">
  <link href="https://fonts.googleapis.com/css?family=Gloria+Hallelujah|Neucha" rel="stylesheet">
  
</head>
<body>
<div id = "wrap">
  
  <%= render 'shared/nav' %><!--Navbar-->
  <br>
  
  <div class = "container">
    <div class = "row">
      <div class = "col-xs-12 col-md-10 col-md-offset-1">
        <%= render 'shared/flash_messages' %>
        <%= yield %>
      </div>
    </div>
  </div>
  
</div><!-- Wrap -->

<%= render 'shared/foot' %><!--Footer-->

</body>
</html>
